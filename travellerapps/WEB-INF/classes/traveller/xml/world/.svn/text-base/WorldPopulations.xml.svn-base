<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="worldPopulations" class="traveller.world.WorldPopulations">
		<property name="populations">
			<map>
				<entry key="0">
					<bean class="traveller.world.WorldPopulation">
						<property name="digit" value="0" />
						<property name="population" value="None" />
						<property name="range" value="0" />
						<property name="description" value="No Population" />
					</bean>
				</entry>
				<entry key="1">
					<bean class="traveller.world.WorldPopulation">
						<property name="digit" value="1" />
						<property name="population" value="Few" />
						<property name="range" value="1" />
						<property name="description" value="A tiny farmstead or a single family." />
					</bean>
				</entry>
				<entry key="2">
					<bean class="traveller.world.WorldPopulation">
						<property name="digit" value="2" />
						<property name="population" value="Hundreds" />
						<property name="range" value="100" />
						<property name="description" value="A village." />
					</bean>
				</entry>
				<entry key="3">
					<bean class="traveller.world.WorldPopulation">
						<property name="digit" value="3" />
						<property name="population" value="Thousands" />
						<property name="range" value="1000" />
						<property name="description" value="Large Village" />
					</bean>
				</entry>
				<entry key="4">
					<bean class="traveller.world.WorldPopulation">
						<property name="digit" value="4" />
						<property name="population" value="Tens of Thousands" />
						<property name="range" value="10000" />
						<property name="description" value="Small Town" />
					</bean>
				</entry>
				<entry key="5">
					<bean class="traveller.world.WorldPopulation">
						<property name="digit" value="5" />
						<property name="population" value="Hundreds of Thousands" />
						<property name="range" value="100000" />
						<property name="description" value="Average City" />
					</bean>
				</entry>
				<entry key="6">
					<bean class="traveller.world.WorldPopulation">
						<property name="digit" value="6" />
						<property name="population" value="Millions" />
						<property name="range" value="1000000" />
						<property name="description" value="Average to Large City" />
					</bean>
				</entry>
				<entry key="7">
					<bean class="traveller.world.WorldPopulation">
						<property name="digit" value="7" />
						<property name="population" value="Tens of Millions" />
						<property name="range" value="10000000" />
						<property name="description" value="Large City" />
					</bean>
				</entry>
				<entry key="8">
					<bean class="traveller.world.WorldPopulation">
						<property name="digit" value="8" />
						<property name="population" value="Hundreds of Millions" />
						<property name="range" value="100000000" />
						<property name="description" value="Several Large Cities" />
					</bean>
				</entry>
				<entry key="9">
					<bean class="traveller.world.WorldPopulation">
						<property name="digit" value="9" />
						<property name="population" value="Billions" />
						<property name="range" value="1000000000" />
						<property name="description" value="Present day Earth" />
					</bean>
				</entry>
				<entry key="10">
					<bean class="traveller.world.WorldPopulation">
						<property name="digit" value="10" />
						<property name="population" value="Tens of Billions" />
						<property name="range" value="10000000000" />
						<property name="description" value="High Population world" />
					</bean>
				</entry>
				<entry key="11">
					<bean class="traveller.world.WorldPopulation">
						<property name="digit" value="11" />
						<property name="population" value="Hundreds of Billions" />
						<property name="range" value="100000000000" />
						<property name="description" value="Incredibly crowded world" />
					</bean>
				</entry>
				<entry key="12">
					<bean class="traveller.world.WorldPopulation">
						<property name="digit" value="12" />
						<property name="population" value="Trillions" />
						<property name="range" value="1000000000000" />
						<property name="description" value="World-city" />
					</bean>
				</entry>
			</map>
		</property>
	</bean>
</beans>