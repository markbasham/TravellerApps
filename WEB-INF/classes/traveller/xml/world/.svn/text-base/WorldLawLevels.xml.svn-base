<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="worldLawLevels" class="traveller.world.WorldLawLevels">
		<property name="lawLevels">
			<map>
				<entry key="0">
					<bean class="traveller.world.WorldLawLevel">
						<property name="digit" value="0" />
						<property name="illegalPossesions">
							<map>
								<entry key-ref="Weapons" value="No restrictions" />
								<entry key-ref="Drugs" value="No restrictions" />
								<entry key-ref="Information" value="INo restrictions" />
								<entry key-ref="Technology" value="No restrictions" />
								<entry key-ref="Travellers" value="No restrictions" />
								<entry key-ref="Psionics" value="No restrictions" />
							</map>
						</property>
					</bean>
				</entry>
				<entry key="1">
					<bean class="traveller.world.WorldLawLevel">
						<property name="digit" value="1" />
						<property name="illegalPossesions">
							<map>
								<entry key-ref="Weapons"
									value="Poison gas, explosives, undetectable weapons, WMD" />
								<entry key-ref="Drugs" value="Highly adictive and dangerous narcotics" />
								<entry key-ref="Information" value="Intellct programs" />
								<entry key-ref="Technology"
									value="Dangerous technologies such as nanotechnology" />
								<entry key-ref="Travellers"
									value="Visitors must contact planetery authorities by radio, landing is permitted anywhere" />
								<entry key-ref="Psionics" value="Dangerous Tallents must be registerd" />
							</map>
						</property>
					</bean>
				</entry>
				<entry key="2">
					<bean class="traveller.world.WorldLawLevel">
						<property name="digit" value="2" />
						<property name="illegalPossesions">
							<map>
								<entry key-ref="Weapons"
									value="Portable energy weapons(Except ship mounted weapons)" />
								<entry key-ref="Drugs" value="Highly adictive narcotics" />
								<entry key-ref="Information" value="Agent programs" />
								<entry key-ref="Technology" value="Alien technology" />
								<entry key-ref="Travellers"
									value="Visitors must report passenger manifest, landing is permitted anywhere" />
								<entry key-ref="Psionics"
									value="All psionic powers must be registered; use of dangerous powers forbidden" />
							</map>
						</property>
					</bean>
				</entry>
				<entry key="3">
					<bean class="traveller.world.WorldLawLevel">
						<property name="digit" value="3" />
						<property name="illegalPossesions">
							<map>
								<entry key-ref="Weapons" value="Heavy Weapons" />
								<entry key-ref="Drugs" value="Combat Drugs" />
								<entry key-ref="Information" value="Intrusion programs" />
								<entry key-ref="Technology" value="TL15 items" />
								<entry key-ref="Travellers"
									value="Landing only at starport or other authorised sites" />
								<entry key-ref="Psionics"
									value="Use of telepathy restricted to government approved telepaths" />
							</map>
						</property>
					</bean>
				</entry>
				<entry key="4">
					<bean class="traveller.world.WorldLawLevel">
						<property name="digit" value="3" />
						<property name="illegalPossesions">
							<map>
								<entry key-ref="Weapons" value="Light assult weapons and submachine guns" />
								<entry key-ref="Drugs" value="Addictive narcotics" />
								<entry key-ref="Information" value="Security programs" />
								<entry key-ref="Technology" value="TL13 items" />
								<entry key-ref="Travellers" value="Landing only at starport" />
								<entry key-ref="Psionics"
									value="Use of tleportation and clairvoyance restricted" />
							</map>
						</property>
					</bean>
				</entry>
				<entry key="5">
					<bean class="traveller.world.WorldLawLevel">
						<property name="digit" value="5" />
						<property name="illegalPossesions">
							<map>
								<entry key-ref="Weapons" value="Personal concealable weapons" />
								<entry key-ref="Drugs" value="Anagathics" />
								<entry key-ref="Information" value="Expert programs" />
								<entry key-ref="Technology" value="TL11 items" />
								<entry key-ref="Travellers"
									value="Citezens must register offworld travel, visitors must register all buisness" />
								<entry key-ref="Psionics"
									value="Use of all psionic powers restricted to government psonicists" />
							</map>
						</property>
					</bean>
				</entry>
				<entry key="6">
					<bean class="traveller.world.WorldLawLevel">
						<property name="digit" value="6" />
						<property name="illegalPossesions">
							<map>
								<entry key-ref="Weapons"
									value="All firearms except shotguns and stunners; carrying weapons discouraged" />
								<entry key-ref="Drugs" value="Fast and Slow drugs" />
								<entry key-ref="Information" value="Recent news from offworld" />
								<entry key-ref="Technology" value="TL9 items" />
								<entry key-ref="Travellers"
									value="visits discouraged; excessive contact with citizens forbidden" />
								<entry key-ref="Psionics" value="Possesion of psionic drugs banned" />
							</map>
						</property>
					</bean>
				</entry>
				<entry key="7">
					<bean class="traveller.world.WorldLawLevel">
						<property name="digit" value="7" />
						<property name="illegalPossesions">
							<map>
								<entry key-ref="Weapons" value="Shotguns" />
								<entry key-ref="Drugs" value="All narcotics" />
								<entry key-ref="Information"
									value="Library programs, unfiltered data about other worlds. Free speach curtailed" />
								<entry key-ref="Technology" value="TL7 items" />
								<entry key-ref="Travellers"
									value="Citezens may not leave planet, visitors may not leave starport" />
								<entry key-ref="Psionics" value="Use of psionics forbidden" />
							</map>
						</property>
					</bean>
				</entry>
				<entry key="8">
					<bean class="traveller.world.WorldLawLevel">
						<property name="digit" value="8" />
						<property name="illegalPossesions">
							<map>
								<entry key-ref="Weapons" value="All bladed weapons, stunners" />
								<entry key-ref="Drugs" value="Medicinal drugs" />
								<entry key-ref="Information"
									value="Information technology, any non-critical data from offworld, personal media" />
								<entry key-ref="Technology" value="TL5 items" />
								<entry key-ref="Travellers" value="Landing permitted only to imperial agents" />
								<entry key-ref="Psionics" value="Psionic-related technology banned" />
							</map>
						</property>
					</bean>
				</entry>
				<entry key="9">
					<bean class="traveller.world.WorldLawLevel">
						<property name="digit" value="9" />
						<property name="illegalPossesions">
							<map>
								<entry key-ref="Weapons" value="Any Weapons" />
								<entry key-ref="Drugs" value="All drugs" />
								<entry key-ref="Information" value="Any data from offworld, no free press" />
								<entry key-ref="Technology" value="TL3 items" />
								<entry key-ref="Travellers" value="No offworlders permitted" />
								<entry key-ref="Psionics" value="All psionics" />
							</map>
						</property>
					</bean>
				</entry>
			</map>
		</property>
	</bean>
</beans>