<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="worldHydrographics" class="traveller.world.WorldHydrographics">
		<property name="hydrographics">
			<map>
				<entry key="0">
					<bean class="traveller.world.WorldHydrographic">
						<property name="digit" value="0" />
						<property name="percentage" value="0%-5%" />
						<property name="description" value="Desert World" />
					</bean>
				</entry>
				<entry key="1">
					<bean class="traveller.world.WorldHydrographic">
						<property name="digit" value="1" />
						<property name="percentage" value="6%-15%" />
						<property name="description" value="Dry World" />
					</bean>
				</entry>
				<entry key="2">
					<bean class="traveller.world.WorldHydrographic">
						<property name="digit" value="2" />
						<property name="percentage" value="16%-25%" />
						<property name="description" value="A few small seas" />
					</bean>
				</entry>
				<entry key="3">
					<bean class="traveller.world.WorldHydrographic">
						<property name="digit" value="3" />
						<property name="percentage" value="26%-35%" />
						<property name="description" value="Small seas and Oceans" />
					</bean>
				</entry>
				<entry key="4">
					<bean class="traveller.world.WorldHydrographic">
						<property name="digit" value="4" />
						<property name="percentage" value="36%-45%" />
						<property name="description" value="Wet world" />
					</bean>
				</entry>
				<entry key="5">
					<bean class="traveller.world.WorldHydrographic">
						<property name="digit" value="5" />
						<property name="percentage" value="46%-55%" />
						<property name="description" value="Large Oceans" />
					</bean>
				</entry>
				<entry key="6">
					<bean class="traveller.world.WorldHydrographic">
						<property name="digit" value="6" />
						<property name="percentage" value="56%-65%" />
						<property name="description" value="Many Large Oceans" />
					</bean>
				</entry>
				<entry key="7">
					<bean class="traveller.world.WorldHydrographic">
						<property name="digit" value="7" />
						<property name="percentage" value="66%-75%" />
						<property name="description" value="Earth like world" />
					</bean>
				</entry>
				<entry key="8">
					<bean class="traveller.world.WorldHydrographic">
						<property name="digit" value="8" />
						<property name="percentage" value="76%-85%" />
						<property name="description" value="Water world" />
					</bean>
				</entry>
				<entry key="9">
					<bean class="traveller.world.WorldHydrographic">
						<property name="digit" value="9" />
						<property name="percentage" value="86%-95%" />
						<property name="description"
							value="Only a few small islands and archipelagoes" />
					</bean>
				</entry>
				<entry key="10">
					<bean class="traveller.world.WorldHydrographic">
						<property name="digit" value="10" />
						<property name="percentage" value="96%-100%" />
						<property name="description" value="Almost entirely water" />
					</bean>
				</entry>

			</map>
		</property>
	</bean>
</beans>